{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#", 
    "contentVersion": "6.0.4.0", 
    "parameters": {
        "adminUsername": {
            "value": "azureuser"
        }, 
        "authenticationType": {
            "value": "password"
        }, 
        "adminPasswordOrKey": {
            "value": "${bigip_admin_password}"
        }, 
        "dnsLabel": {
            "value": "${bigip_dns_label}"
        }, 
        "instanceName": {
            "value": "f5vm01"
        }, 
        "instanceType": {
            "value": "Standard_DS2_v2"
        }, 
        "imageName": {
            "value": "Best1Gbps"
        }, 
        "bigIpVersion": {
            "value": "13.1.100000"
        }, 
        "vnetName": {
            "value": "${bigip_vnet_name}"
        }, 
        "vnetResourceGroupName": {
            "value": "${bigip_vnet_resource_group_name}"
        }, 
        "mgmtSubnetName": {
            "value": "${bigip_mgmt_subnet_name}"
        }, 
        "mgmtIpAddress": {
            "value": "${bigip_mgmt_ip_address}"
        }, 
        "avSetChoice": {
            "value": "CREATE_NEW"
        }, 
        "declarationUrl": {
            "value": "NOT_SPECIFIED"
        }, 
        "ntpServer": {
            "value": "0.pool.ntp.org"
        }, 
        "timeZone": {
            "value": "UTC"
        }, 
        "customImage": {
            "value": "OPTIONAL"
        }, 
        "restrictedSrcAddress": {
            "value": "*"
        }, 
        "tagValues": {
            "value": {
                "application": "APP", 
                "cost": "COST", 
                "environment": "ENV", 
                "group": "GROUP", 
                "owner": "OWNER"
            }
        }, 
        "allowUsageAnalytics": {
            "value": "Yes"
        }
    }
}